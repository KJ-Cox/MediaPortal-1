          <scrollbarwidth>25</scrollbarwidth>
          <explicitlyEnableScrollLabel>yes</explicitlyEnableScrollLabel>
          <unfocusedAlpha applyToAll="true">130</unfocusedAlpha>
				</control>
				<control>
